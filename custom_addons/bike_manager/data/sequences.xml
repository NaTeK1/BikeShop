<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_bike_sale_order" model="ir.sequence">
            <field name="name">Séquence des commandes de vente</field>
            <field name="code">bike.sale.order</field>
            <field name="prefix">SO</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="active">True</field>
        </record>

        <record id="seq_bike_rental" model="ir.sequence">
            <field name="name">Séquence des locations</field>
            <field name="code">bike.rental</field>
            <field name="prefix">RNT</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="active">True</field>
        </record>

        <record id="seq_bike_product" model="ir.sequence">
            <field name="name">Séquence des produits</field>
            <field name="code">bike.product</field>
            <field name="prefix">BIKE-</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="active">True</field>
        </record>

    </data>
</odoo>
